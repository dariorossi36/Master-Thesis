      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Georgios", "'"}], " ", "way"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"BChorrule", "=", 
       RowBox[{"BCSbckHor", "//.", "MODEL"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ruBChor", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"BChorrule", "//.", "coupConst"}], "/.", 
          RowBox[{
           SubscriptBox["fi", "0"], "->", "phic0"}]}], "/.", 
         RowBox[{
          SubscriptBox["ai", "1"], "->", "rh0"}]}], "/.", 
        RowBox[{"rh", "->", "rh0"}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"BOU", "=", 
