Cell[TextData[{
 "I\[CloseCurlyQuote]m interested in the simplest case where the potential Vf \
= 0. Moreover the perturbEQS has a constant value \[Alpha] which can be set \
to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "/", "2"}], TraditionalForm]],ExpressionUUID->
  "4d73e272-24f7-4a9f-b7d9-8907fa08d9c6"],
 " for the whole analysis: in fact it varies depending on the metric used for \
the background field integration, i.e.  if the metric functions are defined \
as exp(\[CapitalUpsilon]) or exp(2\[CapitalUpsilon]), etc."
}], "Text",
 CellChangeTimes->{{3.9045617146662188`*^9, 3.904561718499957*^9}, 
   3.905162737699082*^9},ExpressionUUID->"109dfa29-8376-456d-9be6-\
4c6a39eb102e"],

Cell[TextData[{
 "The equation we need is the following: - ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[PartialD]", "2"], "\[CurlyPhi]1"}], "/", 
    RowBox[{"\[PartialD]", 
     SuperscriptBox["r", "2"]}]}], TraditionalForm]],ExpressionUUID->
  "1ac47136-d73a-4046-a86b-4a6b74e063e9"],
 "  + ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "(", "r", ")"}], TraditionalForm]],ExpressionUUID->
  "9591f750-c59e-43c0-a287-ab6b2bfcdae2"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[PartialD]", "2"], "\[CurlyPhi]1"}], "/", 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["t", "2"]}]}], " ", "+", " ", 
    RowBox[{"k", "(", "r", ")"}]}], TraditionalForm]],ExpressionUUID->
  "006713fa-1e92-499b-be09-2ac562399c6e"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[PartialD]", "\[CurlyPhi]1"}], "/", 
      RowBox[{"\[PartialD]", "r"}]}], " ", "+", 
     RowBox[{
      RowBox[{"p", "(", "r", ")"}], "\[CurlyPhi]1"}]}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"a5e1c41b-72f0-4276-928a-280aa4cf07ee"],
