Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"WRONSKsch", "[", 
     RowBox[{
     "\[Eta]0_", ",", "\[Lambda]0_", ",", " ", "\[Omega]0_", ",", " ", "xm_", 
      ",", "\[Epsilon]0_"}], "]"}], ":=", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Finding", " ", 
       SuperscriptBox["\[CurlyPhi]", 
        RowBox[{"(", "-", ")"}]], " ", "and", " ", 
       SuperscriptBox["\[CurlyPhi]", 
        RowBox[{"(", "+", ")"}]], " ", "with", " ", "the", " ", "PINTEG", " ",
        "function"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"out", "=", 
       RowBox[{"PINTEGsch", "[", 
        RowBox[{
        "1", ",", "\[Eta]0", ",", "\[Lambda]0", ",", "ruleCoupling", ",", 
         "\[Omega]0", ",", "xm", ",", "\[Epsilon]0"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"phimin", "=", 
       RowBox[{"out", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"phiplu", "=", 
       RowBox[{"out", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Dphimin", "=", 
       RowBox[{"out", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Dphiplu", "=", 
       RowBox[{"out", "[", 
        RowBox[{"[", "4", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Wrons1", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            FractionBox["phimin", "Dphimin"], "-", 
            FractionBox["phiplu", "Dphiplu"]}], ")"}], "/.", 
          RowBox[{"x", "->", "xm"}]}], ")"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"Georgios", " ", "definition"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Wrons2", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"phimin", "*", "Dphiplu"}], "-", 
            RowBox[{"phiplu", "*", "Dphimin"}]}], ")"}], "/.", 
          RowBox[{"x", "->", "xm"}]}], ")"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"Canonic", " ", "definition"}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "product", " ", "of", " ", "the", " ", "derivatives", " ", "at", " ", 
        "the", " ", "matching", " ", "point"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"prod", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"Dphimin", "*", "Dphiplu"}], ")"}], "/.", 
          RowBox[{"x", "->", "xm"}]}], ")"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
       "Wrons1", ",", "\[IndentingNewLine]", "Wrons2", ",", 
        "\[IndentingNewLine]", "prod"}], "}"}]}], "\[IndentingNewLine]", 
     ")"}]}], ";"}], "\[IndentingNewLine]"}]], "Input",
